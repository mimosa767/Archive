<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Chanel Demos" verbose="0" parallel="tests" thread-count="100" group-by-instances="true">
    <listeners>
        <listener class-name="com.quantum.listeners.QuantumReportiumListener"/>
        <!--<listener class-name="com.quantum.listeners.TestListener" />-->
    </listeners>

    <test name="Galaxy Note 20" enabled="true">
        <parameter name="driver.name" value="perfectoDriver"/>
        <parameter name="driver.capabilities.deviceName" value="RF8N82SBA6H"/>
        <parameter name="driver.capabilities.appPackage" value="com.gamma.scan"/>
        <parameter name="driver.capabilities.startActivity" value="com.gamma.scan/com.gamma.barcodeapp.ui.BarcodeCaptureActivity" />
        <parameter name="driver.capabilities.driverClass" value="io.appium.java_client.android.AndroidDriver"/>
        <parameter name="env.resources" value="src/main/resources/android"/>
        <groups>
            <run>
                <include name="@ChanelScanQRKakao"/>
            </run>
        </groups>
        <classes>
            <class name="com.qmetry.qaf.automation.step.client.gherkin.GherkinScenarioFactory"/>
        </classes>
    </test>

